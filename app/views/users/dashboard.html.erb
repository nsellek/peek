<div id="Dashboard">

	<div class="md-content">
		<h1>Welcome to the Dashboard</h1>
		<div id="fields">

			<h2>Your Houses</h2>
			<h3>Click a house to view members</h3>

			<% @user.houses.each do |house| %>
			<div id="houseLI">
				<%= image_tag("home-black-silhouette2.png", alt: "House Icon") %>  <%= house.name.split.map(&:capitalize)*' ' %> | <%= link_to 'add face', user_house_pictures_upload_path(current_user.id, house.id) %> | <%= link_to 'access house', user_house_pictures_matchPage_path(current_user.id, house.id) %> | <%= link_to 'lock house', lock_path %>
			</div>
			<% end %>

			<%= link_to 'Add new house', new_user_house_path(current_user.id) %>

		</div>
	</div>

</div>
