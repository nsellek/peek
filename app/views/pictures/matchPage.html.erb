<h1>Pictrue to Match</h1>

<%= form_for @picture, url: user_house_match_path, html: {multipart: true} do |f| %>
  <p>
    <%= f.file_field :image, type: :file, multiple: true %>
  </p>

  <%= f.submit 'Send pictures' %>
<% end %>

<%= link_to 'back to profile', controller: 'users', action: 'dashboard' %>