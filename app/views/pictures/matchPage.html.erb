<div id="Match">

  <div class="md-content">
    <h1>Picture to Match</h1>
    <div id="fields">

      <%= form_for @picture, url: user_house_match_path, html: {multipart: true} do |f| %>

      <p><%= f.file_field :image, type: :file, multiple: true %>

      <p><%= f.submit 'Send pictures' %>
      <% end %>

      <p><%= link_to 'back to profile', controller: 'users', action: 'dashboard' %>
    </div>
  </div>

</div>
